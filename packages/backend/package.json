{
  "name": "grype",
  "displayName": "Grype",
  "description": "Unofficial grype & syft integration for Podman Desktop",
  "version": "0.1.0-next",
  "icon": "icon.png",
  "publisher": "axel7083",
  "type": "module",
  "license": "Apache-2.0",
  "engines": {
    "podman-desktop": ">=1.25.0"
  },
  "main": "./dist/extension.cjs",
  "scripts": {
    "build": "vite build",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch --coverage",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "watch": "vite --mode development build -w"
  },
  "contributes": {
    "menus": {
      "dashboard/image": [
        {
          "command": "syft.analysis",
          "title": "Analyse Image"
        }
      ]
    },
    "commands": [
      {
        "command": "syft.analysis",
        "title": "Syft: analyse Image"
      }
    ]
  },
  "devDependencies": {
    "@podman-desktop/extension-grype-core-api": "workspace:^",
    "@podman-desktop/podman-extension-api": "^1.25.1",
    "@podman-desktop/api": "^1.25.1",
    "@types/node": "^24",
    "@types/adm-zip": "^0.5.7",
    "@types/tar": "^6.1.13",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "zod": "^4.3.6",
    "@octokit/rest": "^22.0.1",
    "adm-zip": "^0.5.12",
    "tar": "^7.4.3"
  }
}
