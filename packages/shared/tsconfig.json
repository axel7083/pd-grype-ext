{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "preserveValueImports": false,
    "skipLibCheck": true,
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": true,
    "paths": {
      "/@generated/*": ["./generated/*"],
    }
  },
  "include": ["src/**/*.d.ts", "src/**/*.ts", "src/**/*.js", "types/*.d.ts", "../../types/**/*.d.ts"],
  "exclude": ["vite.config.ts", "vitest.config.ts"]
}
